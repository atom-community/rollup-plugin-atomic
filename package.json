  "keywords": [
    "rollup",
    "plugin",
    "atomic",
    "js",
    "javascript",
    "ts",
    "typescirpt",
    "coffee",
    "coffeescript",
    "json",
    "css",
    "babel",
    "wasm",
    "as",
    "assemblyscript",
